{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "ThisIsASecretKeyForJwtTokenPleaseChangeIt",
    "Issuer": "YourApp",
    "Audience": "YourAppUsers",
    "ExpiresMinutes": 60
  },
  "ConnectionStrings": {
    "DefaultConnection": "User Id=<用户名>;Password=<密码>;Data Source=<ip>:<port>/<数据库服务名>"
  }
}
