{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Email": {
    "SmtpServer": "<你的SMTP服务器地址>",
    "Port": "587",
    "SenderEmail": "<你的SMTP邮箱地址>",
    "SenderPassword": "<你的SMTP邮箱密码>",
    "EnableSsl": "true",
    "ExpiresMinutes": 10,
    "SenderName": "<邮件的落款名称>默认使用WeHub"
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "ThisIsASecretKeyForJwtTokenPleaseChangeIt",
    "Issuer": "YourApp",
    "Audience": "YourAppUsers",
    "ExpiresMinutes": 30,
    "LongExpiresMinutes": 43200
  },
  "ConnectionStrings": {
    "DefaultConnection": "User Id=<用户名>;Password=<密码>;Data Source=<ip>:<port>/<数据库服务名>"
  }
}
